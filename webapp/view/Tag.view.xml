<mvc:View controllerName="tc.tagCloud.controller.Tag" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	xmlns:cust="tc.tagCloud.control" xmlns="sap.m" id="tagID">
	<App>
		<pages>
			<Page id="page" title="Blogs">
				<content>
					<cust:Custbox src1="https://rydoowebsite.cdn.prismic.io/rydoowebsite%2Fc9116796-b200-4c19-8582-74e486efabaa_sap-logo.svg" prop1="Sap"
						choose="onChooseTag"></cust:Custbox>
					<cust:Custbox src1="images/imgStack.jpg" prop1="Stackoverflow" choose="onChooseTagStack"></cust:Custbox>
					<cust:Custbox src1="images/imgApple.jpg" prop1="Apple" choose="onChooseTag"></cust:Custbox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>